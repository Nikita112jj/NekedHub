    loadstring(game:HttpGet("https://raw.githubusercontent.com/infyiff/backup/main/dex.lua"))()
end})

-- ===== Функции =====
TabUniversal:CreateToggle({Name = "Inf Jump", Callback = function(state)
    local UserInputService = game:GetService("UserInputService")
    if state then
        _G.InfJump = UserInputService.JumpRequest:Connect(function()
            game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
        end)
    else
        if _G.InfJump then _G.InfJump:Disconnect() end
    end
end})

-- ✅ Фикснутый Noclip
TabUniversal:CreateToggle({Name = "Noclip", Callback = function(state)
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local LocalPlayer = Players.LocalPlayer

    if state then
        _G.NoclipLoop = RunService.Stepped:Connect(function()
            if LocalPlayer.Character then
                for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
                    if v:IsA("BasePart") and v.CanCollide == true then
                        v.CanCollide = false
                    end
                end
            end
        end)
    else
        if _G.NoclipLoop then
            _G.NoclipLoop:Disconnect()
            _G.NoclipLoop = nil
        end
    end
end})

-- Скорость
local speed = 16
TabUniversal:CreateInput({Name = "Напишите скорость", PlaceholderText = "Напишите скорость", RemoveTextAfterFocusLost = false, Callback = function(text)
    speed = tonumber(text) or 16
end})

TabUniversal:CreateButton({Name = "Подтвердить скорость", Callback = function()
    game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = speed
end})

TabUniversal:CreateToggle({Name = "Loop Speed", Callback = function(state)
    if state then
        _G.SpeedLoop = game:GetService("RunService").Stepped:Connect(function()
            game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = speed
        end)
    else
        if _G.SpeedLoop then _G.SpeedLoop:Disconnect() end
    end
end})
